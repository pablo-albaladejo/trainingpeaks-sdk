{
  "name": "trainingpeaks-sdk",
  "version": "1.0.2",
  "description": "TypeScript SDK for TrainingPeaks API integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run --exclude='**/*.integ-test.ts'",
    "test:unit": "vitest run --exclude='**/*.integ-test.ts'",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:watch": "vitest --exclude='**/*.integ-test.ts'",
    "test:coverage": "vitest run --coverage --exclude='**/*.integ-test.ts'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "precommit": "npm run format:check && npm run lint && npm run build && npm test",
    "clean": "rm -rf dist",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "pre-release": "npm run clean && npm run test:coverage && npm run lint && npm run format:check && npm run build"
  },
  "keywords": [
    "trainingpeaks",
    "api",
    "sdk",
    "typescript",
    "fitness"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@faker-js/faker": "^9.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^16.4.7",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "playwright": "^1.49.0",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.36.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "form-data": "^4.0.1"
  }
}
