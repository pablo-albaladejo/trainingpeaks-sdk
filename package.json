{
  "name": "trainingpeaks-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for TrainingPeaks API integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run --exclude='**/*.integ-test.ts'",
    "test:unit": "vitest run --exclude='**/*.integ-test.ts'",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:watch": "vitest --exclude='**/*.integ-test.ts'",
    "test:coverage": "vitest run --coverage --exclude='**/*.integ-test.ts'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "precommit": "npm run build && npm test",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "trainingpeaks",
    "api",
    "sdk",
    "typescript",
    "fitness"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "playwright": "^1.49.0",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "form-data": "^4.0.1"
  }
}
